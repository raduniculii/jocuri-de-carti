<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Jocuri de Carti</title>

    <link href="~/lib/bootstrap-4.3.1/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/lib/bootstrap-datepicker3/css/bootstrap-datepicker3.css" rel="stylesheet" />
    <link href="~/lib/fontawesome-free-5.7.2-web/css/all.min.css" rel="stylesheet" />
    @RenderSection("Styles", required: false)
</head>
<body>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2019 - Jocuri de Carti</p>
        </footer>
    </div>

    <script src="~/lib/jquery-3.3.1/js/jquery.min.js"></script>
    <script src="~/lib/bootstrap-4.3.1/js/bootstrap.bundle.js"></script>
    <script src="~/lib/bootstrap-datepicker3/js/bootstrap-datepicker.min.js"></script>
    <script src="~/lib/bootstrap-datepicker3/js/bootstrap-datepicker.ro.min.js"></script>
    <script>
        $(function () {
            $('[data-toggle="tooltip"]').tooltip()
        });
    </script>

    @RenderSection("Scripts", required: false)
</body>
</html>
